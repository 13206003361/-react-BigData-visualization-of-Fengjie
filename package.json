{
  "private": true,
  "homepage": "./",
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/pro-layout": "^5.0.19",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.2.26",
    "axios": "^0.21.0",
    "echarts": "^5.0.0",
    "echarts-for-react": "^2.0.16",
    "flv.js": "^1.5.0",
    "lint-staged": "^10.0.7",
    "lodash.flow": "^3.5.0",
    "moment": "^2.29.1",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-countup": "^4.3.3",
    "react-dnd": "^11.1.3",
    "react-dnd-html5-backend": "^11.1.3",
    "react-dom": "^16.12.0",
    "umi": "^3.2.26",
    "umi-request": "^1.3.5",
    "video.js": "^7.10.2",
    "videojs-flash": "^2.2.1",
    "yorkie": "^2.0.0"
  }
